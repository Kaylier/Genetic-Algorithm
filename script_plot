#! /bin/gnuplot

filename0 = "output/bench_population_size/2/stats"
plot filename0 using 1:3 with lines linetype rgb "#000000ff" title ''
replot filename0 using 1:2:4 with filledcurves closed linetype rgb "#880000ff" title "2"

filename1 = "output/bench_population_size/3/stats"
replot filename1 using 1:3 with lines linetype rgb "#0000ff00" title ''
replot filename1 using 1:2:4 with filledcurves closed linetype rgb "#8800ff00" title "3"

filename2 = "output/bench_population_size/4/stats"
replot filename2 using 1:3 with lines linetype rgb "#00ff0000" title ''
replot filename2 using 1:2:4 with filledcurves closed linetype rgb "#88ff0000" title "4"

#pause -1

